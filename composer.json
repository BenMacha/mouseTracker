    "autoload": {
        "psr-4": { "benmacha\\mousetracker\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
